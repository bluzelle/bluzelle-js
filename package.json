{
  "name": "bluzelle",
  "version": "0.3.0",
  "description": "Bluzelle Database for JavaScript",
  "main": "lib/bluzelle-node.js",
  "browser": "lib/bluzelle-web.js",
  "scripts": {
    "setup-daemon": "cp -R ./test-daemon/configs/. ./test-daemon/daemon-build/output/",
    "test-node": "npm run setup-daemon; daemonIntegration=true port=50000 address=localhost mocha src/main.test.js --exit --timeout 8000",
    "test-browser": "cd src; webpack main.test.js -o main.test.pack.js --mode=\"development\"; open mocha.html",
    "build": "webpack"
  },
  "devDependencies": {
    "async-wait-until": "^1.2.4",
    "fs": "^0.0.1-security",
    "mocha": "^5.0.4",
    "webpack": "^4.3.0",
    "webpack-cli": "^2.0.13",
    "webpack-shell-plugin": "^0.5.0"
  },
  "author": "Monty Thibault, John Lam",
  "license": "ISC",
  "dependencies": {
    "google-protobuf": "^3.5.0",
    "hash.js": "^1.1.5",
    "isomorphic-ws": "^4.0.1",
    "lodash": "^4.17.10",
    "websocket": "^1.0.25",
    "ws": "^6.0.0"
  }
}
